{
	"name": "@taknepoidet/eslint-config-standard",
	"version": "1.0.0",
	"description": "Конфигурация ESLint",
	"author": "TakNePoidet <TakNePoidet@yandex.ru>",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"eslint",
		"eslint-config",
		"standard"
	],
	"main": "index.js",
	"files": [
		"rules/**",
		"index.js",
		".eslintrc.js"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/TakNePoidet/taknepoidet-config.git",
		"directory": "packages/eslint"
	},
	"scripts": {
		"test": "eslint test/**{.ts,.js,.vue}",
		"lint": "eslint ./**{.ts,.js,.vue,.json} --fix && prettier --write ./**{.ts,.js,.vue}",
		"prepublishOnly": "node ./generate.js"
	},
	"peerDependencies": {
		"react": "^17.0.2"
	},
	"dependencies": {
		"@babel/preset-typescript": "^7.13.0",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"@vue/eslint-config-airbnb": "^5.3.0",
		"@vue/eslint-config-typescript": "^7.0.0",
		"eslint": "^7.26.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-import": "^2.23.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-prettier-vue": "^3.0.0",
		"eslint-plugin-promise": "^4.3.1",
		"eslint-plugin-unicorn": "^32.0.1",
		"eslint-plugin-vue": "^7.9.0",
		"prettier": "^2.3.0",
		"typescript": "^4.2.4",
		"vue": "^3.0.11",
		"vue-eslint-parser": "^7.6.0"
	},
	"devDependencies": {
		"deepmerge": "^1.5.2",
		"lodash": "^4.17.21",
		"pre-commit": "^1.2.2",
		"rimraf": "^3.0.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"pre-commit": [
		"lint"
	]
}
