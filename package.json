{
	"name": "taknepoidet-config",
	"version": "1.0.0",
	"author": "TakNePoidet <TakNePoidet@yandex.ru>",
	"workspaces": [
		"packages/*"
	],
	"files": [],
	"scripts": {
		"lint": "eslint --fix",
		"prettier": "prettier --ignore-unknown --write .",
		"beautifier": "npm run lint && npm run prettier"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"pre-commit": [
		"beautifier"
	],
	"devDependencies": {
		"@taknepoidet-config/eslint-config": "file:packages/eslint",
		"@taknepoidet-config/prettier": "file:packages/prettier",
		"lerna": "6.0.3",
		"pre-commit": "1.2.2"
	},
	"peerDependencies": {
		"react": "17.0.2",
		"vue": "3.2.22"
	}
}
